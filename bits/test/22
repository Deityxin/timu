#include <bits/stdc++.h>
using namespace std;
int main() {
    static int dp[21][21][21];
    for (int a = 0; a <= 20; ++a) {
        for (int b = 0; b <= 20; ++b) {
            for (int c = 0; c <= 20; ++c) {
                if (a == 0 || b == 0 || c == 0) dp[a][b][c] = 1;
                else if (a < b && b < c)
                    dp[a][b][c] = dp[a][b][c - 1] + dp[a][b - 1][c - 1] - dp[a][b - 1][c];
                else
                    dp[a][b][c] = dp[a - 1][b][c] + dp[a - 1][b - 1][c] + dp[a - 1][b][c - 1] - dp[a - 1][b - 1][c - 1];
            }
        }
    }
    long long x, y, z;
    while (cin >> x >> y >> z) {
        if (x == -1 && y == -1 && z == -1) break;
        int a = x < 0 ? 0 : (x > 20 ? 20 : (int)x);
        int b = y < 0 ? 0 : (y > 20 ? 20 : (int)y);
        int c = z < 0 ? 0 : (z > 20 ? 20 : (int)z);
        cout << "w(" << x << ", " << y << ", " << z << ") = " << dp[a][b][c] << '\n';
    }
    return 0;
}